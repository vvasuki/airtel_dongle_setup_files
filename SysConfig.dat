[Install]
DefDir=airtel
AppTitle=airtel
AppName=airtel.exe
Publisher=Huawei Technologies Co.,Ltd
URLInfoAbout=http://www.huawei.com
HelpLink=http://mobile.huawei.com
RealName=airtel.app

[DRIVER]
WLAN=0

[FinishPage]
run=\eap\wifiman_inistall.exe
runtext=

[execwait]
uninstall=\eap\wifiman_unistall.exe

[shortcut]
dest1=airtel.exe
name1=airtel
dest2=uninst.exe
name2=Uninstall
usermanual=User Manual
usermanualfile=

[regedit]
TCPWindowSize=129940
reboot=false

[customize]
operator=Bharti_C284
simtype=SIM/USIM
uimtype=UIM
FindDevice=false
LaunchDashboard=false
runasadmin=false
IPv6Install=false
driverFilterList=-nosms

[language]
name1=English

[System]
FileVersion=1
VRVersion=23
VVersion=200
RVersion=003
BuildVersion=015
DebugVersion=02
SPVersion=11
CustomizeVersion=284

[SPLASH]
SHOW_PROMPT=NO
PROMPT_MESSAGE=PromptInfo.exe
LANGPATH=PromptInfo
LANGFILE=English.dat

[PNPCTRL]
EXEC_REWIND=YES
DEFAULT_TIMEOUT=10
WIN2000_TIMEOUT=10

[EHIDS_PRODUCT_VERSION]
COMMENT=
RUN_INSTALL_APP=YES
RUN_CLIENT_APP=YES
L_REG_PATH=SOFTWARE\Huawei technologies\airtel
APP_FILE_NAME=airtel.exe
APP_INSTALL_PATH=airtel
VALUE=23.015.02.11.284

[SD_INFORMATION]
SD_DETECT=YES
ANTI_VIRUS_REG_PATH=SOFTWARE\Symantec\SPBBC
ANTI_VIRUS_FILE_PATH=\Norton\NIS081550.exe
REMIND_SENTENCE=Remind you to install antivirus software?

[SD_THIRDPARTY_SETUP]
SD_THIRD_SETUP=NO
SD_THIRD_FILE_PATH=PUBLIC
SD_THIRD_FILE_NAME=HuaweiProtectorSuite.dmg
SD_RUN_THIRD_FILE_ONCE=YES

[VIRCDROM_INFORMATION]
ANTI_VIRUS_TREND_EXE=\AntiVirus\TmSync.exe

[APP_PATH]
VALUE=/Applications/airtel.app
RUN_PROCESS_DLG=YES

